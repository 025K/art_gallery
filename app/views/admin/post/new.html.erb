<h1>画像投稿フォーム</h1>

<%= form_with model: @post, url: admin_post_index_path, local:true do |f| %>
  <h4>画像</h4>
  <%= f.file_field :images, multiple: true %>
  <h4>ショップ名</h4>
  <%= f.text_field :title %>
  <h4>説明</h4>
  <%= f.text_area :caption %>
  <h4>ジャンル</h4>
  <%= f.select :post_genre, [['お知らせ'], ['漫画'], ['イラスト'], ['グッズ']] %>
  <%= f.submit '投稿' %>
<% end %>